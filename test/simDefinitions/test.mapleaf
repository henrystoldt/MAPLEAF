RocketName{
    class               Rocket
    positionX           (0 0 0) # m
    initialDirection    (0 0 1) 
    velocity            (10 0 50) #m/s
    angularVelocity     (0 0 0) # rad/s

    Sustainer{
        class           Stage
        stageNumber      0 #First and only stage

        UpperBodyTubeMass{
            class           Mass
            mass            50 
            position       (0 0 0)
            cg             (0 0 0)
        }
    }
}